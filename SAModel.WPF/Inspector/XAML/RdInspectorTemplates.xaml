            <ComboBoxItem Content="Material_Diffuse" Tag="{x:Static chunk:ChunkType.Material_Diffuse}"/>
            <ComboBoxItem Content="Material_Ambient" Tag="{x:Static chunk:ChunkType.Material_Ambient}"/>
            <ComboBoxItem Content="Material_DiffuseAmbient" Tag="{x:Static chunk:ChunkType.Material_DiffuseAmbient}"/>
            <ComboBoxItem Content="Material_Specular" Tag="{x:Static chunk:ChunkType.Material_Specular}"/>
            <ComboBoxItem Content="Material_DiffuseSpecular" Tag="{x:Static chunk:ChunkType.Material_DiffuseSpecular}"/>
            <ComboBoxItem Content="Material_AmbientSpecular" Tag="{x:Static chunk:ChunkType.Material_AmbientSpecular}"/>
            <ComboBoxItem Content="Material_DiffuseAmbientSpecular" Tag="{x:Static chunk:ChunkType.Material_DiffuseAmbientSpecular}"/>
            <ComboBoxItem Content="Material_Bump" Tag="{x:Static chunk:ChunkType.Material_Bump}"/>
            <ComboBoxItem Content="Material_Diffuse2" Tag="{x:Static chunk:ChunkType.Material_Diffuse2}"/>
            <ComboBoxItem Content="Material_Ambient2" Tag="{x:Static chunk:ChunkType.Material_Ambient2}"/>
            <ComboBoxItem Content="Material_DiffuseAmbient2" Tag="{x:Static chunk:ChunkType.Material_DiffuseAmbient2}"/>
            <ComboBoxItem Content="Material_Specular2" Tag="{x:Static chunk:ChunkType.Material_Specular2}"/>
            <ComboBoxItem Content="Material_DiffuseSpecular2" Tag="{x:Static chunk:ChunkType.Material_DiffuseSpecular2}"/>
            <ComboBoxItem Content="Material_AmbientSpecular2" Tag="{x:Static chunk:ChunkType.Material_AmbientSpecular2}"/>
            <ComboBoxItem Content="Material_DiffuseAmbientSpecular2" Tag="{x:Static chunk:ChunkType.Material_DiffuseAmbientSpecular2}"/>
            <ComboBoxItem Content="Vertex_VertexSH" Tag="{x:Static chunk:ChunkType.Vertex_VertexSH}"/>

            <ComboBoxItem Content="Vertex_VertexDiffuse8" Tag="{x:Static chunk:ChunkType.Vertex_VertexDiffuse8}"/>
            <ComboBoxItem Content="Vertex_VertexUserAttributes" Tag="{x:Static chunk:ChunkType.Vertex_VertexUserAttributes}"/>
            <ComboBoxItem Content="Vertex_VertexNinjaAttributes" Tag="{x:Static chunk:ChunkType.Vertex_VertexNinjaAttributes}"/>
            <ComboBoxItem Content="Vertex_VertexDiffuseSpecular5" Tag="{x:Static chunk:ChunkType.Vertex_VertexDiffuseSpecular5}"/>
            <ComboBoxItem Content="Vertex_VertexDiffuseSpecular4" Tag="{x:Static chunk:ChunkType.Vertex_VertexDiffuseSpecular4}"/>
            <ComboBoxItem Content="Vertex_VertexDiffuseSpecular16" Tag="{x:Static chunk:ChunkType.Vertex_VertexDiffuseSpecular16}"/>
            <ComboBoxItem Content="Vertex_VertexNormal" Tag="{x:Static chunk:ChunkType.Vertex_VertexNormal}"/>
            <ComboBoxItem Content="Vertex_VertexNormalDiffuse8" Tag="{x:Static chunk:ChunkType.Vertex_VertexNormalDiffuse8}"/>
            <ComboBoxItem Content="Vertex_VertexNormalUserAttributes" Tag="{x:Static chunk:ChunkType.Vertex_VertexNormalUserAttributes}"/>
            <ComboBoxItem Content="Vertex_VertexNormalNinjaAttributes" Tag="{x:Static chunk:ChunkType.Vertex_VertexNormalNinjaAttributes}"/>
            <ComboBoxItem Content="Vertex_VertexNormalDiffuseSpecular5" Tag="{x:Static chunk:ChunkType.Vertex_VertexNormalDiffuseSpecular5}"/>
            <ComboBoxItem Content="Vertex_VertexNormalDiffuseSpecular4" Tag="{x:Static chunk:ChunkType.Vertex_VertexNormalDiffuseSpecular4}"/>
            <ComboBoxItem Content="Vertex_VertexNormalDiffuseSpecular16" Tag="{x:Static chunk:ChunkType.Vertex_VertexNormalDiffuseSpecular16}"/>

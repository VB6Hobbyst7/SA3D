<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:objEnums="clr-namespace:SATools.SAModel.ObjectData;assembly=SAModel"
    xmlns:attachEnums="clr-namespace:SATools.SAModel.ModelData;assembly=SAModel"
    xmlns:gcenums="clr-namespace:SATools.SAModel.ModelData.GC;assembly=SAModel"
    xmlns:num="http://gu.se/NumericInput"
    xmlns:sub="clr-namespace:SATools.SAModel.WPF.Inspector.XAML.SubControls"
    xmlns:local="clr-namespace:SATools.SAModel.WPF.Inspector.XAML"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:numerics="clr-namespace:System.Numerics;assembly=System.Numerics"
    xmlns:structs="clr-namespace:SATools.SAModel.Structs;assembly=SAModel"
    xmlns:base="clr-namespace:SATools.SAWPF.BaseViewModel;assembly=SAWPF"
    xmlns:sawpf="clr-namespace:SATools.SAWPF;assembly=SAWPF"
    xmlns:basic="clr-namespace:SATools.SAModel.ModelData.BASIC;assembly=SAModel"
    xmlns:chunk="clr-namespace:SATools.SAModel.ModelData.CHUNK;assembly=SAModel">

    <!--Note: datatypes do not reflect datacontext type, but the Inspector binding type-->
    
    <!--#region Base templates -->
    
    <DataTemplate x:Key="/" />

    <DataTemplate x:Key="OpenButton">
        <sub:DetailButton Content="{Binding DetailName}" local:InspectorBinding.PropName="DetailObject">
            <sub:DetailButton.Style>
                <Style TargetType="{x:Type sub:DetailButton}">
                    <Style.Triggers>
                        <Trigger Property="DetailObject" Value="{x:Null}">
                            <Setter Property="IsEnabled" Value="False"/>
                        </Trigger>
                    </Style.Triggers>
                </Style>
            </sub:DetailButton.Style>
        </sub:DetailButton>
    </DataTemplate>

    <DataTemplate x:Key="ActionButton" DataType="{x:Type base:RelayCommand}">
        <Button Content="{Binding DetailName}" local:InspectorBinding.PropName="Command"/>
    </DataTemplate>

    <DataTemplate x:Key="OnlyHex">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <TextBlock Text="0x" TextAlignment="Right" VerticalAlignment="Center" Margin="4,0,0,0" ToolTip="Hexadecimal prependix"/>
            <ContentPresenter Grid.Column="1" Name="Hex" Content="{Binding }" ContentTemplateSelector="{DynamicResource TemplateSelector}"/>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="HybridHex">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="2"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>


            <ContentPresenter Name="NoHex" Content="{Binding }" ContentTemplateSelector="{DynamicResource TemplateSelector}"/>
            <Border BorderThickness="0" Grid.Column="1" Background="#2444"/>
            <ContentPresenter Grid.Column="2" Content="{Binding }" ContentTemplate="{StaticResource OnlyHex}"/>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="Boolean" DataType="{x:Type sys:Boolean}">
        <CheckBox Margin="2" local:InspectorBinding.PropName="IsChecked" ToolTip="{Binding Tooltip}" VerticalAlignment="Center">
            <CheckBox.Style>
                <Style TargetType="{x:Type CheckBox}">
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding IsReadOnly}" Value="True">
                            <Setter Property="IsEnabled" Value="False"/>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </CheckBox.Style>
        </CheckBox>
    </DataTemplate>
    
    <DataTemplate x:Key="String" DataType="{x:Type sys:String}">
        <TextBox BorderThickness="0" Padding="2" local:InspectorBinding.PropName="Text"
                 IsReadOnly="{Binding IsReadOnly}" ToolTip="{Binding Tooltip}"
                 Background="Transparent"/>
    </DataTemplate>

    <DataTemplate x:Key="Color" DataType="{x:Type structs:Color}">
        <sub:UcColor local:InspectorBinding.PropName="Value"/>
    </DataTemplate>

    <!--#endregion-->
    
    <!--#region Number templates-->

    <Style x:Key="BaseNumberStyle" 
           TargetType="{x:Type num:BaseBox}">
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="2"/>
        <Setter Property="ValidationTrigger" Value="PropertyChanged"/>
        <Setter Property="Background" Value="Transparent"/>
    </Style>

    <DataTemplate x:Key="Vector2" DataType="{x:Type numerics:Vector2}">
        <sub:UcVector2 local:InspectorBinding.PropName="Value" BaseBoxStyle="{StaticResource BaseNumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="Vector3" DataType="{x:Type numerics:Vector3}">
        <sub:UcVector3 local:InspectorBinding.PropName="Value" BaseBoxStyle="{StaticResource BaseNumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="Vector4" DataType="{x:Type numerics:Vector4}">
        <sub:UcVector4 local:InspectorBinding.PropName="Value" BaseBoxStyle="{StaticResource BaseNumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="Quaternion" DataType="{x:Type numerics:Quaternion}">
        <sub:UcQuaternion local:InspectorBinding.PropName="Value" BaseBoxStyle="{StaticResource BaseNumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="Matrix4x4" DataType="{x:Type numerics:Matrix4x4}">
        <sub:UcMatrixDisplay local:InspectorBinding.PropName="Value" BaseBoxStyle="{StaticResource BaseNumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="Bounds" DataType="{x:Type structs:Bounds}">
        <sub:UcBounds local:InspectorBinding.PropName="Value" BaseBoxStyle="{StaticResource BaseNumberStyle}"/>
    </DataTemplate>

    <Style x:Key="NumberStyle" 
           TargetType="{x:Type num:BaseBox}"
           BasedOn="{StaticResource BaseNumberStyle}">
        <Setter Property="IsReadOnly" Value="{Binding IsReadOnly}"/>
        <Setter Property="local:InspectorBinding.PropName" Value="Value"/>
    </Style>

    <DataTemplate x:Key="Byte" DataType="{x:Type sys:Byte}">
        <sawpf:ByteBox ToolTip="Unsigned Byte ( 8 bits )"
            Style="{StaticResource NumberStyle}"/>
    </DataTemplate>
    
    <DataTemplate x:Key="Int64" DataType="{x:Type sys:Int64}">
        <sawpf:LongBox ToolTip="Signed 64 bit integer ( Long )"
            Style="{StaticResource NumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="UInt64" DataType="{x:Type sys:UInt64}">
        <sawpf:ULongBox ToolTip="Unsigned 64 bit integer ( ULong )"
            Style="{StaticResource NumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="Int32" DataType="{x:Type sys:Int32}">
        <num:IntBox ToolTip="Signed 32 bit integer ( Int )"
            Style="{StaticResource NumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="UInt32" DataType="{x:Type sys:UInt32}">
        <sawpf:UIntBox ToolTip="Unsigned 32 bit integer ( UInt )"
            Style="{StaticResource NumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="Int16" DataType="{x:Type sys:Int16}">
        <num:ShortBox ToolTip="Signed 16 bit integer ( Short )"
            Style="{StaticResource NumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="UInt16" DataType="{x:Type sys:UInt16}">
        <sawpf:UShortBox ToolTip="Unsigned 16 bit integer ( UShort )"
            Style="{StaticResource NumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="Double" DataType="{x:Type sys:Double}">
        <num:DoubleBox ToolTip="64 bit floating point number ( Double )"
            Style="{StaticResource NumberStyle}"/>
    </DataTemplate>

    <DataTemplate x:Key="Single" DataType="{x:Type sys:Single}">
        <num:FloatBox ToolTip="32 bit floating point number ( Single )"
            Style="{StaticResource NumberStyle}"/>
    </DataTemplate>

    <!--#endregion-->

    <!--#region Hexadecimal templates-->

    <Style x:Key="HexNumberStyle" 
           TargetType="{x:Type num:BaseBox}">
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="0,2,2,2"/>
        <Setter Property="IsReadOnly" Value="{Binding IsReadOnly}"/>
        <Setter Property="ToolTip" Value="{Binding Tooltip}"/>
        <Setter Property="ValidationTrigger" Value="PropertyChanged"/>
        <Setter Property="TextAlignment" Value="Left"/>
        <Setter Property="local:InspectorBinding.PropName" Value="Value"/>
        <Setter Property="IsEnabled" Value="False"/>
        <!--Well, we gotta wait for a fix... till then, this is disabled-->

        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Background" Value="Transparent"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <DataTemplate x:Key="Hex:Byte" DataType="{x:Type sys:Byte}">
        <sawpf:ByteBox ToolTip="Unsigned Byte ( 8 bits )"
            Style="{StaticResource HexNumberStyle}"
            MaxLength="2" StringFormat="X2"/>
    </DataTemplate>

    <DataTemplate x:Key="Hex:Int64" DataType="{x:Type sys:Int64}">
        <sawpf:LongBox ToolTip="Hexadecimal signed 64 bit integer ( Long )"
            Style="{StaticResource HexNumberStyle}" 
            MaxLength="16" StringFormat="X16"/>
    </DataTemplate>

    <DataTemplate x:Key="Hex:UInt64" DataType="{x:Type sys:UInt64}">
        <sawpf:ULongBox ToolTip="Hexadecimal unsigned 64 bit integer ( ULong )"
            Style="{StaticResource HexNumberStyle}" 
            MaxLength="16" StringFormat="X16"/>
    </DataTemplate>

    <DataTemplate x:Key="Hex:Int32" DataType="{x:Type sys:Int32}">
        <num:IntBox ToolTip="Hexadecimal signed 32 bit integer ( Int )"
            Style="{StaticResource HexNumberStyle}" 
            MaxLength="8" StringFormat="X8"/>
    </DataTemplate>

    <DataTemplate x:Key="Hex:UInt32" DataType="{x:Type sys:UInt32}">
        <sawpf:UIntBox ToolTip="Hexadecimal unsigned 32 bit integer ( UInt )"
            Style="{StaticResource HexNumberStyle}" 
            MaxLength="8" StringFormat="X8"/>
    </DataTemplate>

    <DataTemplate x:Key="Hex:Int16" DataType="{x:Type sys:Int16}">
        <num:ShortBox ToolTip="Hexadecimal signed 16 bit integer ( Short )"
            Style="{StaticResource HexNumberStyle}" 
            MaxLength="4" StringFormat="X4"/>
    </DataTemplate>

    <DataTemplate x:Key="Hex:UInt16" DataType="{x:Type sys:UInt16}">
        <sawpf:UShortBox ToolTip="Hexadecimal unsigned 16 bit integer ( ZShort )"
            Style="{StaticResource HexNumberStyle}" 
            MaxLength="4" StringFormat="X4"/>
    </DataTemplate>


    <!--#endregion-->

    <!--The enums and flags are manually edited in so we can create tooltips and custom names-->
    <!--It'd be a bit weird to add in display attributes to a data library-->

    <!--#region Enums-->

    <Style x:Key="EnumComboBoxStyle" TargetType="{x:Type sub:EnumComboBox}">
        <Setter Property="local:InspectorBinding.PropName" Value="Value"/>

        <Style.Triggers>
            <DataTrigger Binding="{Binding IsReadOnly}" Value="True">
                <Setter Property="IsReadOnly" Value="True"/>
            </DataTrigger>

            <Trigger Property="IsReadOnly" Value="True">
                <Setter Property="ItemContainerStyle">
                    <Setter.Value>
                        <Style TargetType="{x:Type ComboBoxItem}">
                            <Setter Property="IsEnabled" Value="False"/>
                            <Setter Property="ToolTipService.ShowOnDisabled" Value="True"/>
                        </Style>
                    </Setter.Value>
                </Setter>
            </Trigger>
        </Style.Triggers>
    </Style>

    <DataTemplate x:Key="LandtableFormat" DataType="{x:Type objEnums:LandtableFormat}">
        <sub:EnumComboBox ToolTip="Landtable Data Format" Style="{StaticResource EnumComboBoxStyle}">
            <ComboBoxItem Content="SA1"    Tag="{x:Static objEnums:LandtableFormat.SA1}"       ToolTip="Sonic Adventure 1 LandTable (BASIC)"/>
            <ComboBoxItem Content="SADX"   Tag="{x:Static objEnums:LandtableFormat.SADX}"      ToolTip="Sonic Adventure DX LandTable (BASIC)"/>
            <ComboBoxItem Content="SA2"    Tag="{x:Static objEnums:LandtableFormat.SA2}"       ToolTip="Sonic Adventure 2 LandTable (CHUNK / BASIC)"/>
            <ComboBoxItem Content="SA2B"   Tag="{x:Static objEnums:LandtableFormat.SA2B}"      ToolTip="Sonic Adventure 2: Battle LandTable (GC / BASIC)"/>
            <ComboBoxItem Content="Buffer" Tag="{x:Static objEnums:LandtableFormat.Buffer}"    ToolTip="SAModel library intermediate LandTable (Buffer)"/>
        </sub:EnumComboBox>
    </DataTemplate>

    <DataTemplate x:Key="AttachFormat" DataType="{x:Type attachEnums:AttachFormat}">
        <sub:EnumComboBox ToolTip="Attach format" Style="{StaticResource EnumComboBoxStyle}">
            <ComboBoxItem Content="BASIC"   Tag="{x:Static attachEnums:AttachFormat.BASIC}"     ToolTip="BASIC mesh format"/>
            <ComboBoxItem Content="CHUNK"   Tag="{x:Static attachEnums:AttachFormat.CHUNK}"     ToolTip="CHUNK mesh format"/>
            <ComboBoxItem Content="GC"      Tag="{x:Static attachEnums:AttachFormat.GC}"        ToolTip="Gamecube mesh format"/>
            <ComboBoxItem Content="Buffer"  Tag="{x:Static attachEnums:AttachFormat.Buffer}"    ToolTip="SAModel library intermediate format"/>
        </sub:EnumComboBox>
    </DataTemplate>

    <DataTemplate x:Key="VertexAttribute" DataType="{x:Type gcenums:VertexAttribute}">
        <sub:EnumComboBox ToolTip="Vertex type format" Style="{StaticResource EnumComboBoxStyle}">
            <ComboBoxItem Content="Position Matrix ID" Tag="{x:Static gcenums:VertexAttribute.PositionMatrixId}" ToolTip="Position Matrices along with their ID (?)"/>
            <ComboBoxItem Content="Position"    Tag="{x:Static gcenums:VertexAttribute.Position}"   ToolTip="Position data"/>
            <ComboBoxItem Content="Normal"      Tag="{x:Static gcenums:VertexAttribute.Normal}"     ToolTip="Normal data"/>
            <ComboBoxItem Content="Color 0"     Tag="{x:Static gcenums:VertexAttribute.Color0}"     ToolTip="1st color data"/>
            <ComboBoxItem Content="Color 1"     Tag="{x:Static gcenums:VertexAttribute.Color1}"     ToolTip="2nd color data"/>
            <ComboBoxItem Content="Texcoord 0"  Tag="{x:Static gcenums:VertexAttribute.Tex0}"       ToolTip="1st texture coordinate data"/>
            <ComboBoxItem Content="Texcoord 1"  Tag="{x:Static gcenums:VertexAttribute.Tex1}"       ToolTip="2nd texture coordinate data"/>
            <ComboBoxItem Content="Texcoord 2"  Tag="{x:Static gcenums:VertexAttribute.Tex2}"       ToolTip="3rd texture coordinate data"/>
            <ComboBoxItem Content="Texcoord 3"  Tag="{x:Static gcenums:VertexAttribute.Tex3}"       ToolTip="4th texture coordinate data"/>
            <ComboBoxItem Content="Texcoord 4"  Tag="{x:Static gcenums:VertexAttribute.Tex4}"       ToolTip="5th texture coordinate data"/>
            <ComboBoxItem Content="Texcoord 5"  Tag="{x:Static gcenums:VertexAttribute.Tex5}"       ToolTip="6th texture coordinate data"/>
            <ComboBoxItem Content="Texcoord 6"  Tag="{x:Static gcenums:VertexAttribute.Tex6}"       ToolTip="7th texture coordinate data"/>
            <ComboBoxItem Content="Texcoord 7"  Tag="{x:Static gcenums:VertexAttribute.Tex7}"       ToolTip="8th texture coordinate data"/>
        </sub:EnumComboBox>
    </DataTemplate>

    <DataTemplate x:Key="DataType" DataType="{x:Type gcenums:DataType}">
        <sub:EnumComboBox ToolTip="Data storage type" Style="{StaticResource EnumComboBoxStyle}">
            <ComboBoxItem Content="Unsigned8"   Tag="{x:Static gcenums:DataType.Unsigned8}"     ToolTip="Equal to byte"/>
            <ComboBoxItem Content="Signed8"     Tag="{x:Static gcenums:DataType.Signed8}"       ToolTip="Equal to sbyte"/>
            <ComboBoxItem Content="Unsigned16"  Tag="{x:Static gcenums:DataType.Unsigned16}"    ToolTip="Equal to ushort"/>
            <ComboBoxItem Content="Signed16"    Tag="{x:Static gcenums:DataType.Signed16}"      ToolTip="Equal to short"/>
            <ComboBoxItem Content="Float32"     Tag="{x:Static gcenums:DataType.Float32}"       ToolTip="Equal to float"/>
            <ComboBoxItem Content="RGB565"      Tag="{x:Static gcenums:DataType.RGB565}"        ToolTip="RGB565 struct (Length: 2)"/>
            <ComboBoxItem Content="RGB8"        Tag="{x:Static gcenums:DataType.RGB8}"          ToolTip="RGB8 struct (length: 4)"/>
            <ComboBoxItem Content="RGBX8"       Tag="{x:Static gcenums:DataType.RGBX8}"         ToolTip="RGBX8 struct (length: 4)"/>
            <ComboBoxItem Content="RGBA4"       Tag="{x:Static gcenums:DataType.RGBA4}"         ToolTip="RGBA4 struct (length: 2)"/>
            <ComboBoxItem Content="RGBA6"       Tag="{x:Static gcenums:DataType.RGBA6}"         ToolTip="RGBA6 struct (length: 3)"/>
            <ComboBoxItem Content="RGBA8"       Tag="{x:Static gcenums:DataType.RGBA8}"         ToolTip="RGBA8 struct (length: 4)"/>
        </sub:EnumComboBox>
    </DataTemplate>

    <DataTemplate x:Key="StructType" DataType="{x:Type gcenums:StructType}">
        <sub:EnumComboBox ToolTip="Component structure type" Style="{StaticResource EnumComboBoxStyle}">
            <ComboBoxItem Content="Position XY"   Tag="{x:Static gcenums:StructType.PositionXY}"     ToolTip="2D Position"/>
            <ComboBoxItem Content="Position XYZ"   Tag="{x:Static gcenums:StructType.PositionXYZ}"     ToolTip="3D Position"/>
            <ComboBoxItem Content="Normal XYZ"   Tag="{x:Static gcenums:StructType.NormalXYZ}"     ToolTip="3D Normal"/>
            <ComboBoxItem Content="Normal NBT"   Tag="{x:Static gcenums:StructType.NormalNBT}"     ToolTip="3D Normal (NBT)"/>
            <ComboBoxItem Content="Normal NBT3"   Tag="{x:Static gcenums:StructType.NormalNBT3}"     ToolTip="3D Normal (NBT3)"/>
            <ComboBoxItem Content="Color RGB"   Tag="{x:Static gcenums:StructType.ColorRGB}"     ToolTip="RGB Color"/>
            <ComboBoxItem Content="Color RGBA"   Tag="{x:Static gcenums:StructType.ColorRGBA}"     ToolTip="RGBA Color"/>
            <ComboBoxItem Content="TexCoord S"   Tag="{x:Static gcenums:StructType.TexCoordS}"     ToolTip="1D Texture Coordinate"/>
            <ComboBoxItem Content="TexCoord ST"   Tag="{x:Static gcenums:StructType.TexCoordST}"     ToolTip="2D Texture Coordinate"/>
        </sub:EnumComboBox>
    </DataTemplate>

    <DataTemplate x:Key="BlendMode" DataType="{x:Type attachEnums:BlendMode}">
        <sub:EnumComboBox ToolTip="Blending mode" Style="{DynamicResource EnumComboBoxStyle}">
            <ComboBoxItem Content="Zero" Tag="{x:Static attachEnums:BlendMode.Zero}"/>
            <ComboBoxItem Content="One" Tag="{x:Static attachEnums:BlendMode.One}"/>
            <ComboBoxItem Content="Other" Tag="{x:Static attachEnums:BlendMode.Other}"/>
            <ComboBoxItem Content="Other Inverted" Tag="{x:Static attachEnums:BlendMode.OtherInverted}"/>
            <ComboBoxItem Content="SrcAlpha" Tag="{x:Static attachEnums:BlendMode.SrcAlpha}"/>
            <ComboBoxItem Content="SrcAlpha Inverted" Tag="{x:Static attachEnums:BlendMode.SrcAlphaInverted}"/>
            <ComboBoxItem Content="DstAlpha" Tag="{x:Static attachEnums:BlendMode.DstAlpha}"/>
            <ComboBoxItem Content="DstAlpha Inverted" Tag="{x:Static attachEnums:BlendMode.DstAlphaInverted}"/>
        </sub:EnumComboBox>
    </DataTemplate>

    <DataTemplate x:Key="FilterMode" DataType="{x:Type attachEnums:FilterMode}">
        <sub:EnumComboBox ToolTip="Texture Filtering Mode" Style="{DynamicResource EnumComboBoxStyle}">
            <ComboBoxItem Content="PointSampled" Tag="{x:Static attachEnums:FilterMode.PointSampled}"/>
            <ComboBoxItem Content="Bilinear" Tag="{x:Static attachEnums:FilterMode.Bilinear}"/>
            <ComboBoxItem Content="Trilinear" Tag="{x:Static attachEnums:FilterMode.Trilinear}"/>
            <ComboBoxItem Content="Blend" Tag="{x:Static attachEnums:FilterMode.Blend}"/>
        </sub:EnumComboBox>
    </DataTemplate>

    <DataTemplate x:Key="BASICPolyType" DataType="{x:Type basic:BASICPolyType}">
        <sub:EnumComboBox ToolTip="Texture Filtering Mode" Style="{DynamicResource EnumComboBoxStyle}">
            <ComboBoxItem Content="Triangles" Tag="{x:Static basic:BASICPolyType.Triangles}"/>
            <ComboBoxItem Content="Quads" Tag="{x:Static basic:BASICPolyType.Quads}"/>
            <ComboBoxItem Content="NPoly" Tag="{x:Static basic:BASICPolyType.NPoly}"/>
            <ComboBoxItem Content="Strips" Tag="{x:Static basic:BASICPolyType.Strips}"/>
        </sub:EnumComboBox>
    </DataTemplate>

    <DataTemplate x:Key="ChunkType" DataType="{x:Type chunk:ChunkType}">
        <sub:EnumComboBox ToolTip="Chunk content type" Style="{DynamicResource EnumComboBoxStyle}">
            <ComboBoxItem Content="Null" Tag="{x:Static chunk:ChunkType.Null}"/>

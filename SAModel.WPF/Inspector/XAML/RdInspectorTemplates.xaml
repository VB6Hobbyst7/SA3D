            <ComboBoxItem Content="Volume_Polygon3" Tag="{x:Static chunk:ChunkType.Volume_Polygon3}"/>
            <ComboBoxItem Content="Volume_Polygon4" Tag="{x:Static chunk:ChunkType.Volume_Polygon4}"/>
            <ComboBoxItem Content="Volume_Strip" Tag="{x:Static chunk:ChunkType.Volume_Strip}"/>
            <ComboBoxItem Content="Strip_Strip" Tag="{x:Static chunk:ChunkType.Strip_Strip}"/>
            <ComboBoxItem Content="Strip_StripUVN" Tag="{x:Static chunk:ChunkType.Strip_StripUVN}"/>
            <ComboBoxItem Content="Strip_StripUVH" Tag="{x:Static chunk:ChunkType.Strip_StripUVH}"/>
            <ComboBoxItem Content="Strip_StripNormal" Tag="{x:Static chunk:ChunkType.Strip_StripNormal}"/>
            <ComboBoxItem Content="Strip_StripUVNNormal" Tag="{x:Static chunk:ChunkType.Strip_StripUVNNormal}"/>
            <ComboBoxItem Content="Strip_StripUVHNormal" Tag="{x:Static chunk:ChunkType.Strip_StripUVHNormal}"/>
            <ComboBoxItem Content="Strip_StripColor" Tag="{x:Static chunk:ChunkType.Strip_StripColor}"/>
